        "column_F7HnBr": {
          "type": "column",
          "settings": {
            "image_full_width": false,
            "retina": true,
            "title": "1. Prepare your File",
            "text": "<p>The professional standard for file resolution is 300 dpi ( Dots Per Inch or Pixels Per Inch). We advise that you stick to this resolution and set your file up to your desired size.<br/><br/></p>",
            "link_label": "",
            "link": ""
